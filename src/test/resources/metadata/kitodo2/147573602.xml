  <mets:structMap TYPE="LOGICAL">
    <mets:div ADMID="AMD" DMDID="DMDLOG_0000" ID="LOG_0000" LABEL="Tractätgen von denen Jüdischen Fabeln und Aberglauben" ORDERLABEL="Tractätgen von denen Jüdischen Fabeln und Aberglauben" TYPE="monograph">
      <mets:div ID="LOG_0001" TYPE="title_page" />
      <mets:div DMDID="DMDLOG_0001" ID="LOG_0002" LABEL="Vorrede an den Leser." TYPE="preface" />
      <mets:div DMDID="DMDLOG_0002" ID="LOG_0003" LABEL="Fabeln der abergläubischen Juden, so sie bey ihrer Kranckheit , und wenn sie sterben wollen, haben; Dahingegen aber allemahl gewiesen wird, wie wir Christen uns in solchem Fall aufführen und verhalten sollen." TYPE="section">
        <mets:div DMDID="DMDLOG_0003" ID="LOG_0004" LABEL="Zum Ersten." TYPE="section" />
        <mets:div DMDID="DMDLOG_0004" ID="LOG_0005" LABEL="II. Der Jüden ihr Aberglauben, wenn sie in Sterbens-Nöthen ihr Gebeth thun." TYPE="section" />
        <mets:div DMDID="DMDLOG_0005" ID="LOG_0006" LABEL="III. Von dem Aberglauben der Jüden, daß sie sich nicht fürchten, daß sie sterben, wenn sie kranck sind." TYPE="section" />
        <mets:div DMDID="DMDLOG_0006" ID="LOG_0007" LABEL="IV. Fabeln und Aberglauben, so die Jüden haben, wenn sie gestorben sind." TYPE="section" />
        <mets:div DMDID="DMDLOG_0007" ID="LOG_0008" LABEL="V. Ferner folgen noch mehr Ceremonien, und was vor Aberglauben die Juden dabey haben." TYPE="section" />
        <mets:div DMDID="DMDLOG_0008" ID="LOG_0009" LABEL="VI. Haben die Juden noch einen Aberglauben von den Todten, ehe sie aus der Stube getragen werden, der ist dieser:" TYPE="section" />
        <mets:div DMDID="DMDLOG_0009" ID="LOG_0010" LABEL="VII. Ferner die Ceremonien von Reinigung der Todten, was vor grossen Aberglauben sie darbey haben." TYPE="section" />
        <mets:div DMDID="DMDLOG_0010" ID="LOG_0011" LABEL="IIX." TYPE="section" />
        <mets:div DMDID="DMDLOG_0011" ID="LOG_0012" LABEL="IX. Fabeln und Aberglauben, so die Juden bey ihrem Begräbnis haben." TYPE="section" />
        <mets:div DMDID="DMDLOG_0012" ID="LOG_0013" LABEL="XI. Aberglaube der Juden bey dem Gottsacker." TYPE="section" />
        <mets:div DMDID="DMDLOG_0013" ID="LOG_0014" LABEL="XII. Die Ceremonien und Aberglauben, wenn die Juden den Todten ins Grab legen." TYPE="section" />
        <mets:div DMDID="DMDLOG_0014" ID="LOG_0015" LABEL="XIII. Was die aberglaubischen Juden mehr mit den Todten treiben." TYPE="section" />
        <mets:div DMDID="DMDLOG_0015" ID="LOG_0016" LABEL="XIV. Fabeln und Aberglauben bey der Jüden ihrer Trauer." TYPE="section" />
        <mets:div DMDID="DMDLOG_0016" ID="LOG_0017" LABEL="XV. Aberglaube, den die Juden haben, wann sie vom Gottsacker gehen." TYPE="section" />
        <mets:div DMDID="DMDLOG_0017" ID="LOG_0018" LABEL="XVI. Aberglauben, welchen die Jüden haben, wenn sie vom Gottsacker nach Hause kommen." TYPE="section" />
        <mets:div DMDID="DMDLOG_0018" ID="LOG_0019" LABEL="XVII. Der Juden Aberglaube, den sie in Nachfolge der Trauer haben." TYPE="section" />
        <mets:div DMDID="DMDLOG_0019" ID="LOG_0020" LABEL="XVIII." TYPE="section" />
      </mets:div>
    </mets:div>
  </mets:structMap>
