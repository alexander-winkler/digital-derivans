        <mods:mods>
          <mods:titleInfo>
            <mods:title>IX. Fabeln und Aberglauben, so die Juden bey ihrem Begräbnis haben.</mods:title>
          </mods:titleInfo>
        </mods:mods>
      </mets:xmlData>
    </mets:mdWrap>
