  <mets:dmdSec ID="DMDLOG_0002">
    <mets:mdWrap MDTYPE="MODS">
      <mets:xmlData>
        <mods:mods>
          <mods:titleInfo>
            <mods:title>Fabeln der abergläubischen Juden, so sie bey ihrer Kranckheit , und wenn sie sterben wollen, haben; Dahingegen aber allemahl gewiesen wird, wie wir Christen uns in solchem Fall aufführen und verhalten sollen.</mods:title>
          </mods:titleInfo>
        </mods:mods>
      </mets:xmlData>
    </mets:mdWrap>
  </mets:dmdSec>
  <mets:dmdSec ID="DMDLOG_0003">
    <mets:mdWrap MDTYPE="MODS">
      <mets:xmlData>
        <mods:mods>
          <mods:titleInfo>
            <mods:title>Zum Ersten.</mods:title>
